{
  "extends": "next/core-web-vitals",
  "rules": {
    "import/no-anonymous-default-export": 0,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "semi": ["error", "never"],
    "react/display-name": 0,
    "react/no-children-prop": 0
  }
}
